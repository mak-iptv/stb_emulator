{
  "name": "stb-web-emulator-full",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "cd client && npm ci",
    "build": "cd client && npm install && npm run build",
    "start": "node server/index.js",
    "dev": "concurrently \"cd client && npm run dev\" \"nodemon server/index.js\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-fetch": "^2.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}